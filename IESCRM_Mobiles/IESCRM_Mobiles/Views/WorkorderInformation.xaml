                                        <Label  Text=" Dated " FontSize="13" FontAttributes="Bold" TextColor="Black" Grid.Column="0" Grid.Row="2" />
                                        <Label  Text="Notes" FontSize="13" FontAttributes="Bold" TextColor="Black" Grid.Column="1" Grid.Row="2" />
                                        <Label  Text="Entered By" FontSize="13" FontAttributes="Bold"  TextColor="Black" Grid.Column="2" Grid.Row="2" />
                                        <BoxView BackgroundColor="Black" HeightRequest="2" HorizontalOptions="FillAndExpand" Grid.Row="1" Grid.ColumnSpan="3" />
                                        <BoxView BackgroundColor="Black" HeightRequest="2" HorizontalOptions="FillAndExpand" Grid.Row="3" Grid.ColumnSpan="3" />
                                     
                                    </Grid>
                                </ListView.Header>
                                <ListView.ItemTemplate>
                                    <DataTemplate>
                                        <ViewCell>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="25*" />
                                                    <ColumnDefinition Width="25*" />
                                                    <ColumnDefinition Width="25*" />
                                                </Grid.ColumnDefinitions>
                                                <Label  Grid.Column="0" Grid.Row="0" FontSize="10"  TextColor="Black"   Text="{Binding AdminNotesDateTime}"/>
                                                <Label  Grid.Column="1" Grid.Row="0" FontSize="10" TextColor="Black"    Text="{Binding AdminNotes}"/>
                                                <Label  Grid.Column="2" Grid.Row="0" FontSize="11" TextColor="Black"    Text="{Binding EnteredBy}"/>
                                            </Grid>
                                        </ViewCell>
                                    </DataTemplate>
                                </ListView.ItemTemplate>
                            </ListView>
                        </ScrollView>
                    </StackLayout>

                    <StackLayout BackgroundColor="#d82425" Padding="1">
                        <StackLayout  BackgroundColor="#ecd3d4" >
                            <StackLayout.GestureRecognizers>
                                <TapGestureRecognizer Tapped="Tabtecnicians1"/>
                            </StackLayout.GestureRecognizers>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="40*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                                <Label Text="Technicians" FontAttributes="None" TextColor="#3e403f" Grid.Row="0" Grid.Column="1"  Grid.ColumnSpan="2"  HorizontalTextAlignment="Start" VerticalOptions="CenterAndExpand" FontSize="19"/>
                                <ImageButton Source="technician.png" BackgroundColor="#ecd3d4" WidthRequest="30" HeightRequest="30"  HorizontalOptions="Start" VerticalOptions="CenterAndExpand" Grid.Row="0" Grid.ColumnSpan="2"  Grid.Column="0" ></ImageButton>
                                <ImageButton x:Name="bttechni"  Source= "r.png" WidthRequest="20"  HorizontalOptions="End" VerticalOptions="CenterAndExpand" Grid.Row="0" Grid.ColumnSpan="2"  Grid.Column="1" ></ImageButton>
                            </Grid>
                        </StackLayout>
                    </StackLayout>
                    <StackLayout  VerticalOptions="FillAndExpand" x:Name="tabtechnicians" IsVisible="False"  HorizontalOptions="FillAndExpand" >
                        <ScrollView Orientation="Vertical">
                            <ListView  HasUnevenRows="True" x:Name="technicians" BackgroundColor="White" HeightRequest="300">
                                <ListView.Header>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="2"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="25*"/>
                                            <ColumnDefinition Width="25*"/>
                                            <ColumnDefinition Width="25*"/>
                                        </Grid.ColumnDefinitions>
                                        <Label  Text="Assigned Date: " FontSize="13" FontAttributes="Bold" TextColor="Black" Grid.Column="0" Grid.Row="0" />
                                        <Label  Text="Technician Name:" FontSize="13" FontAttributes="Bold" TextColor="Black" Grid.Column="1" Grid.Row="0" />
                                        <Label  Text="Is Active:" FontSize="13" FontAttributes="Bold"  TextColor="Black" Grid.Column="2" Grid.Row="0" />
                                        <BoxView BackgroundColor="Black" HeightRequest="2" HorizontalOptions="FillAndExpand" Grid.Row="1" Grid.ColumnSpan="3" />
                                    </Grid>
                                </ListView.Header>
                                <ListView.ItemTemplate>
                                    <DataTemplate>
                                        <ViewCell>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="25*" />
                                                    <ColumnDefinition Width="25*" />
                                                    <ColumnDefinition Width="25*" />
                                                </Grid.ColumnDefinitions>
                                                <Label  Grid.Column="0" Grid.Row="0" FontSize="13"  TextColor="Black"   Text="{Binding AssignedDate}"/>
                                                <Label  Grid.Column="1" Grid.Row="0" FontSize="13" TextColor="Black"    Text="{Binding UserFirstName}"/>
                                                <Label  Grid.Column="2" Grid.Row="0" FontSize="13" TextColor="Black"    Text="{Binding IsActiveTechnician}"/>
                                            </Grid>
                                        </ViewCell>
                                    </DataTemplate>
                                </ListView.ItemTemplate>
                            </ListView>
                        </ScrollView>
                    </StackLayout>
                   
                    <StackLayout BackgroundColor="#d82425" Padding="1">

                        <StackLayout  BackgroundColor="#ecd3d4" >
                            <StackLayout.GestureRecognizers>
                                <TapGestureRecognizer Tapped="Tabshippingadress1"/>
                            </StackLayout.GestureRecognizers>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="40*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                                <Label Text="Shipping Address" FontAttributes="None" TextColor="#3e403f" Grid.Row="0" Grid.Column="1"  Grid.ColumnSpan="2"  HorizontalTextAlignment="Start" VerticalOptions="CenterAndExpand" FontSize="19"/>
                                <ImageButton Source="delivery.png" BackgroundColor="#ecd3d4" WidthRequest="30" HeightRequest="30"  HorizontalOptions="Start" VerticalOptions="CenterAndExpand" Grid.Row="0" Grid.ColumnSpan="2"  Grid.Column="0" ></ImageButton>
                                <ImageButton x:Name="btshipw" Source= "r.png" WidthRequest="20"  HorizontalOptions="End" VerticalOptions="CenterAndExpand" Grid.Row="0" Grid.ColumnSpan="2"  Grid.Column="1" ></ImageButton>
                            </Grid>
                        </StackLayout>
                    </StackLayout>
                    <StackLayout  VerticalOptions="FillAndExpand" x:Name="tabshippingadress" IsVisible="False"  HorizontalOptions="FillAndExpand" >
                       
                        <ScrollView Orientation="Vertical">
                            <ListView  HasUnevenRows="True" x:Name="tabshippingadds" BackgroundColor="White" HeightRequest="300">
                                <ListView.Header>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="2"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40*"/>
                                            <ColumnDefinition Width="25*"/>
