                                        <Label  Text="Contact Name: " FontSize="13" FontAttributes="Bold" TextColor="Black" Grid.Column="0" Grid.Row="0" />
                                        <Label  Text="Address:" FontSize="13" FontAttributes="Bold" TextColor="Black" Grid.Column="1" Grid.Row="0" />
                                        <Label  Text="City:" FontSize="13" FontAttributes="Bold"  TextColor="Black" Grid.Column="2" Grid.Row="0" />
                                        <Label  Text="Province:" FontSize="13" FontAttributes="Bold"  TextColor="Black" Grid.Column="3" Grid.Row="0" />
