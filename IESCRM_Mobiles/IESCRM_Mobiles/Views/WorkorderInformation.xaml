                                    </Grid>
                                </ListView.Header>
                                <ListView.ItemTemplate>
                                    <DataTemplate>
                                        <ViewCell>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="40*" />
                                                    <ColumnDefinition Width="25*" />
                                                    <ColumnDefinition Width="20*" />
                                                    <ColumnDefinition Width="20*" />
                                                </Grid.ColumnDefinitions>
                                                <Label  Grid.Column="0" Grid.Row="0" FontSize="13"  TextColor="Black"   Text="{Binding ContactName}"/>
                                                <Label  Grid.Column="1" Grid.Row="0" FontSize="13" TextColor="Black"    Text="{Binding Address}"/>
                                                <Label  Grid.Column="2" Grid.Row="0" FontSize="13" TextColor="Black"    Text="{Binding City}"/>
